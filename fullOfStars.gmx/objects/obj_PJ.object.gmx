            <string>/// Movimientos

arriba = keyboard_check(vk_up);
abajo = keyboard_check(vk_down);
derecha = keyboard_check(vk_right);
izquierda = keyboard_check(vk_left);

// Derecha || Izquierda
if (derecha) {
    x += velocidad;
    sprite_index = sprite1_spr_player_right_strip6;
    image_speed = .2;
}
if (izquierda) {
    x -= velocidad;
    sprite_index = sprite2_spr_player_left_strip6;
    image_speed = .2;
}
// Arriba || Abajo
if (arriba) {
    y -= velocidad;
    sprite_index = sprite3_spr_player_up_strip6;
    image_speed = .2;
}
if (abajo) {
    y += velocidad;
    sprite_index = sprite0_spr_player_down_strip6;
    image_speed = .2;
}
if (keyboard_check(vk_nokey)){
    image_speed = 0;
    image_index = 0;
}
</string>
